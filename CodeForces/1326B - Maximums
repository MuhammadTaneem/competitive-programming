#include <bits/stdc++.h>
using namespace std;
#define all(x) (x).begin(),(x).end()
#define endl "\n"
#define fr(x) for(int i=0;i<x;i++)
#define frj(x) for(int j=0;j<x;j++)
#define fr(x) for(int i=x-1;i>=0;i--)
int nxt()
{
    int x;
    cin>>x;
    return x;
}
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int mx=0,cr,t=nxt();
    while(t--)
    {
        cin>>cr;
        cr+=mx;
        mx=max(cr,mx);
        cout<<cr<<" ";
    }
    cout<<endl;
    return 0;
}
