#include <iostream>
 
using namespace std;
 
int main()
{
    long long int a,b,c;
    int d, i;
    cin>>d;
    for(i=0;i<d;i++)
    {
        cin>>a>>b;
        c=a-b;
        if(c>1)
        {
            cout<<"YES"<<endl;
        }
        else
            cout<<"NO"<<endl;
    }
    return 0;
}
