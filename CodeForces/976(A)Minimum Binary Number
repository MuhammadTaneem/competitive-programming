#include <iostream>
#include<string.h>
 
using namespace std;
 
int main()
{
    int a,k,count=0,zero=0;
    string b;
    cin>>a>>b;
    for(int i=0;i<a;i++)
    {
        if(b.at(i)==49)
        {
            count++;
        }
        else{
            zero++;
        }
    }
    if(count>0)
    {
        cout<<1;
    }
    for(int i=0;i<zero;i++)
    {
        cout<<0;
    }
    cout <<endl;
    return 0;
}
