#include <bits/stdc++.h>
using namespace std;

int main()
{
    long long int t;
    scanf("%lld",&t);
    while(t--)
    {
        int a;
        scanf("%d",&a);
        vector<int>arr(a);
        for (auto &it : arr)cin>>it;
        sort(arr.begin(),arr.end());
        for(int i=arr.size()-1;i>=0;i--)
        {
            printf("%d ",arr[i]);
        }
        printf("\n");
    }

    return 0;
}
