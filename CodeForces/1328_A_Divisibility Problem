#include <bits/stdc++.h>
using namespace std;
#define all(x) (x).begin(),(x).end()
#define endl "\n"
#define fr(x) for(int i=0;i<x;i++)
int nxt()
{
    int x;
    cin>>x;
    return x;
}
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t=nxt();
    while(t--)
    {
        int a=nxt(),b=nxt();
        if(a%b==0)
            cout<<0<<endl;
        else{
            int r=a%b;
        cout<<b-r<<endl;
        }
 
 
    }
 
    return 0;
}
