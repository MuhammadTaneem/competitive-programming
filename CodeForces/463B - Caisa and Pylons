#include <bits/stdc++.h>
using namespace std;
#define all(x) (x).begin(),(x).end()
#define endl "\n"
int nxt()
{
    int x;
    cin>>x;
    return x;
}
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    vector<int>v;
    int t=nxt(),c=0;
    for(int i=0;i<t;i++)
    {
        v.push_back(nxt());
    }

    sort(all(v));
    cout<<v[v.size()-1]<<endl;

    return 0;
}
