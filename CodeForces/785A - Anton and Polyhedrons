#include <bits/stdc++.h>
using namespace std;
#define all(x) (x).begin(),(x).end()
#define endl "\n"
#define fr(x) for(int i=0;i<x;i++)
#define frj(x) for(int j=0;j<x;j++)
#define fr(x) for(int i=x-1;i>=0;i--)
int nxt()
{
    int x;
    cin>>x;
    return x;
}
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    string s;
    int t=nxt();
    int n=0;
    while(t--)
    {
        cin>>s;
        if(s=="Tetrahedron")n+=4;
        else if(s=="Cube")n+=6;
        else if(s=="Octahedron")n+=8;
        else if(s=="Dodecahedron")n+=12;
        else n+=20;

    }
    cout<<n<<endl;

    return 0;
}
