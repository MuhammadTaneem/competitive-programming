#include <bits/stdc++.h>
using namespace std;
 
int main()
{
    int t;
    scanf("%d",&t);
    while(t--)
    {
        string a,b,c;
        cin>>a>>b>>c;
        int k=0;
        for(int i=0;i<a.size();i++)
        {
 
            if((a[i]==c[i])||(b[i]==c[i]))
            {
                k++;
            }
 
 
        }
        if(k==(a.size()))
        {
            printf("YES\n");
        }
        else{
            printf("NO\n");
        }
    }
    return 0;
}
