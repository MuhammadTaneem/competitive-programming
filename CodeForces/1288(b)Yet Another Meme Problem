
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    scanf("%d",&n);
    while(n)
    {
        long long int a,b,c=0;
        scanf("%lld%lld",&a,&b);
        for(long long int i=9;i<=b;i=(10*i)+9)
        {
            ++c;
        }
        printf("%lld\n",c*a);
        n--;
 
    }
    return 0;
 
}
//output willbe possible when the b is 9 or 99  or  999 like that
