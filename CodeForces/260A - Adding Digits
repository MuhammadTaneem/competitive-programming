#include<iostream>
using namespace std;
int main()
{
    int a,b,c,d=1;
    scanf("%d%d%d",&a,&b,&c);
    for(int i=0;i<10;i++)
    {
        if((a*10+i)%b==0)
        {
            printf("%d",(a*10)+i);
            string s(c-1,'0');
            cout<<s<<endl;
            d=0;
            break;
        }
    }
    if(d)
    {
         printf("-1\n");
    }
    return 0;
}
