
#include <iostream>
 
using namespace std;
 
int main()
{
    int n;
    cin>>n;
    for(int i=0;i<n;i++)
    {
        int a,b;
        cin>>a>>b;
        if(a>=b||a>3)
        {
            cout<<"YES"<<endl;
        }
        else if(a==2&&b==3)
        {
             cout<<"YES"<<endl;
        }
        else
        cout<<"NO"<<endl;
    }
    return 0;
}
