#include <bits/stdc++.h>
using namespace std;
#define all(x) (x).begin(),(x).end()
#define endl "\n"
#define fr(x) for(int i=0;i<x;i++)
#define frj(x) for(int j=0;j<x;j++)
#define fr(x) for(int i=x-1;i>=0;i--)
int nxt()
{
    int x;
    cin>>x;
    return x;
}
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    string s;
    int two,three,five,six;
    cin>>two>>three>>five>>six;
    int bg=min(two,min(five,six));
    //cout<<bg<<endl;
    int n=min(two-bg,three);
    n=max(n,0);
    cout<<(bg*256)+(32*n)<<endl;
    //cout<<n<<endl;

    return 0;
}
