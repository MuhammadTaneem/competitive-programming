#include <bits/stdc++.h>
using namespace std;

int main()
{
    int arr[5][5],k=0;
    for(int i=0;i<5;i++)
    {
        for(int j=0;j<5;j++)
        {
            scanf("%d",&arr[i][j]);
            if(arr[i][j]==1)
            {
                k=abs(2-i);
                k+=abs(2-j);
            }

        }

    }
    printf("%d\n",k);
    return 0;
}

