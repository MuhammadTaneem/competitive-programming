#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    scanf("%d",&n);
    while(n--)
    {
        long long int  a,b,d,e;
        scanf("%lld%lld",&a,&b);
        long long int c=sqrt(b);
        d=b/c;
        if(d*c<b)
        {
            d+=c;
        }
        else{
            d+=c-1;
        }
        if(d<=a)
        {
          printf("YES\n");
        }
        else
        {
            printf("NO\n");
        }
    }
    return 0;

}
