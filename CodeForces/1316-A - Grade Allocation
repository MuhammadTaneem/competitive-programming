#include <bits/stdc++.h>
using namespace std;
 
int main()
{int n;
    scanf("%d",&n);
    while(n--){
       int a,b,s=0;
    scanf("%d%d",&a,&b);
    for(int i=0;i<a;i++)
    {
        int t;
         scanf("%d",&t);
        s+=t;
    }
    //asdlfldasf
    cout<<min(b,s)<<endl;
    }
 
    return 0;
}
