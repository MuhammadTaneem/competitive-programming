#include <bits/stdc++.h>
using namespace std;
#define all(x) (x).begin(),(x).end()
#define endl "\n"
#define fr(x) for(int i=0;i<x;i++)
int nxt()
{
    int x;
    cin>>x;
    return x;
}
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t=nxt();
    while(t--)
    {
        int n=nxt();
        if(n<2)
        {
            cout<<-1;
        }
        else{
            cout<<23;
        }
 
        for(int j=3;j<=n;j++)
        {
            cout<<9;
        }
        cout<<endl;
    }
 
 
    return 0;
}
