#include<bits/stdc++.h>
using namespace std;
int main()
{
    int arr[129]={0};
    int s=0,i=0;
    char a[100];
    cin>>a;
 
 
    while(a[i]!='\0')
    {
        arr[int(a[i])]++;
        i++;
    }

    for(int i=0;i<129;i++)
    {
        if(arr[i]!='\0')
        {
            s++;
        }
    }
    if(s%2)
    {
        cout<<"IGNORE HIM!"<<endl;
    }
    else{
        cout<<"CHAT WITH HER!"<<endl;
    }
    return 0;
}
