#include <bits/stdc++.h>
using namespace std;
 
int main()
{
    int t;
    scanf("%d",&t);
    while(t--)
    {
        int a,b,c,d,n=0;
        scanf("%d%d%d%d",&a,&b,&c,&d);
        b-=a;
 
        n=b/(c+d);
        if(n*(c+d)==b)
        {
            printf("%d\n",n);
        }
        else
        {
                printf("-1\n");
        }
 
    }
    return 0;
}
 
