#include <iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;
    for(int j=0;j<n;j++)
    {
        int x,s=0;
        cin>>x;
        for(int i=0;i<x;i++)
        {
            int y;
            cin>>y;
            if(y<=2048)
            {
              s+=y;
            }
        }
 
        if(s>=2048)
        {
            cout<<"YES"<<endl;
        }
        else{cout<<"NO"<<endl;}
 
    }
 
    return 0;
}
